    <td>        item[:institution]</td>
    <td>        item[:isbn]</td>
    <td>        item[:location]</td>
    <td>        item[:note]</td>
    <td>        item[:publisher]</td>
    <td>        item[:retrieved]</td>
    <td>        item[:tech]</td>
