<div class="col-sm">
  <%= form_with :url => { :action => "pub_integrate" }, id: "publications" do |form| %>
    <div class="row">
      <p>College: <%= form.select(:college, @colleges, {include_blank: "Select a college"}) %></p>
      <p> Access ID: <%= form.text_field :access_id %></p>
    </div>
    <div class="row">
      <%= render 'buttons_and_code', form: form %>
    </div>
  <% end %>
</div>
<div class="col-sm">
  <h4>RMD Publications Integration Errors:</h4>
  <br/>
  <%= render 'file_download_button', log_path: @publications_log_path %>
</div>