<div class="col-sm">
  <%= form_with :url => { :action => "cv_presentation_integrate" }, id: "cv_presentations" do |form| %>
    <p> <%= label(:cv_presentation_file, text = "CV Presentations CSV") %><%= form.file_field :cv_presentation_file, required: true %> </p>
    <div class="row">
      <%= render 'buttons_and_code', form: form %>
    </div>
  <% end %>
</div>
<div class="col-sm">
  <h4>CV Presentations Integration Errors:</h4>
  <br/>
  <%= render 'file_download_button', log_path: @cv_presentations_log_path %>
</div>