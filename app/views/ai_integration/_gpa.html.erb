<div class="col-sm">
  <h3>GPA Integration</h3>
  <%= form_with :url => { :action => "gpa_integrate" }, id: "gpa" do |form| %>
    <p> <%= label(:gpa_file, text = "GPA Data File") %><%= form.file_field :gpa_file, required: true %> </p>
    <div class="row">
      <%= render 'buttons_and_code', form: form %>
    </div>
  <% end %>
</div>
<div class="col-sm">
  <h4>GPA Integration Errors:</h4>
  <br/>
  <p><%= link_to "Download Error Log", "psu/error_outputs/gpa_errors.log", download: "gpa_errors.log", target: '_blank', class: "btn btn-secondary" %></p>
</div>